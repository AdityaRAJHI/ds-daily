---
    import Layout from '../layouts/Layout.astro';
    ---

    <Layout title="Data Science">
      <main class="max-w-6xl mx-auto px-4 py-12">
        <section class="text-center mb-16">
          <h1 class="text-5xl font-bold text-gray-900 mb-6">
            Data Science
          </h1>
          <p class="text-xl text-gray-600 mb-8 max-w-2xl mx-auto">
            Explore the world of data science with our articles and tutorials.
          </p>
        </section>
        <section class="mt-10">
          <h2 class="text-2xl font-bold text-gray-900 mb-4">Notes</h2>
          <ul class="list-disc list-inside text-gray-700">
            <li>This page provides content related to Data Science.</li>
            <li>It includes articles and tutorials about data science concepts.</li>
          </ul>
        </section>
      </main>
    </Layout>
