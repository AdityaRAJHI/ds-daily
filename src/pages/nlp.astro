---
    import Layout from '../layouts/Layout.astro';
    ---

    <Layout title="Natural Language Processing">
      <main class="max-w-6xl mx-auto px-4 py-12">
        <section class="text-center mb-16">
          <h1 class="text-5xl font-bold text-gray-900 mb-6">
            Natural Language Processing
          </h1>
          <p class="text-xl text-gray-600 mb-8 max-w-2xl mx-auto">
            Dive into the world of NLP with our articles and tutorials.
          </p>
        </section>
        <section class="mt-10">
          <h2 class="text-2xl font-bold text-gray-900 mb-4">Notes</h2>
          <ul class="list-disc list-inside text-gray-700">
            <li>This page provides content related to Natural Language Processing.</li>
            <li>It includes articles and tutorials about NLP concepts and techniques.</li>
          </ul>
        </section>
      </main>
    </Layout>
